{
    "name": "ebalo/easycrud",
    "description": "",
    "keywords": [
        "ebalo",
        "easycrud"
    ],
    "homepage": "https://github.com/ebalo/easycrud",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "ebalo",
            "email": "webdev.ebalo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ebalo\\EasyCRUD\\": "src"
        },
        "files": [
            "src/crud.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Ebalo\\EasyCRUD\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ebalo\\EasyCRUD\\EasyCRUDServiceProvider"
            ]
        }
    }
}
